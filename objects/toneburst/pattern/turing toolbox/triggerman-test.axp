      <object id="patch/object" uuid="a0ee71d48208b71752cbb8c05e55145106ef3946">
         <sDescription></sDescription>
         <author>Johannes Taelman</author>
         <license>BSD</license>
         <helpPatch>disp.axh</helpPatch>
         <inlets/>
