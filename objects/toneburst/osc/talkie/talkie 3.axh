// Mix voiced and unvoiced inlets (code from Factory mix/xfade object)
int64_t a = (int64_t)inlet_unvoicedWave * mix;
a += (int64_t)inlet_voicedWave * ccompl;

// Multiply by synthEnergy value
a *= inlet_synthEnergy;

// Set outlet sample
outlet_wave = a >> 35;

]]></code.srate>
